#include <stdio.h>

// Enum to track the type of data stored in the union
enum Type {
    INTEGER,
    FLOATING_POINT
};

// Union definition
union Data {
	int integer;
	float floating_point;
};

struct Value{
enum Type type;
union Data data;
};

int main() {struct Value var1,var2;
	scanf("%d",&var1.data.integer);
	var1.type= INTEGER;
	while (getchar() !='\n')
		;
	   scanf("%f",&var2.data.floating_point);
	var2.type =FLOATING_POINT;
	if (var1.type == INTEGER){
		printf("%d\n",var1.data.integer);
		
	}
	else if (var1.type == FLOATING_POINT){
		printf("%f\n",var1.data.floating_point);
	}
if (var2.type == INTEGER){
			printf("%d\n",var2.data.integer);

		}
	else if (var2.type == FLOATING_POINT){
		printf("%.1f\n",var2.data.floating_point);
	}
	return 0;
}
